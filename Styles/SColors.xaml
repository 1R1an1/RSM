<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">



    <!-- Border color -->
    <Color x:Key="Point0">#000000</Color>
    <Color x:Key="Point1">#380000</Color>
    <Color x:Key="Point2">#ff0000</Color>

    <Brush x:Key="Point0B">#000000</Brush>
    <Brush x:Key="Point1B">#380000</Brush>
    <Brush x:Key="Point2B">#ff0000</Brush>

    <!-- Button normal color -->
    <Brush x:Key="NormalColorNormal">#252423</Brush>
    <Brush x:Key="NormalColorMouseOver">#2F2D2D</Brush>
    
    <!-- Button download color -->
    <Brush x:Key="DownloadColorNormal">#0081FF</Brush>
    <Brush x:Key="DownloadColorMouseOver">#0065FF</Brush>
    
    <!-- Button jugar color -->
    <Brush x:Key="JugarColorNormal">#00FF1F</Brush>
    <Brush x:Key="JugarColorMouseOver">#00FF5E</Brush>
    
    <!-- Button eliminar color -->
    <Brush x:Key="EliminarColorNormal">#FF0021</Brush>
    <Brush x:Key="EliminarColorMouseOver">#FF0000</Brush>

    <Brush x:Key="Background">#171618</Brush>

</ResourceDictionary>